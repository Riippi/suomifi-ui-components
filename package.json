{
  "name": "suomifi-ui-components",
  "version": "7.0.0-beta.0",
  "description": "Suomi.fi UI component library",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/types/index.d.ts",
  "style": "dist/main.css",
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vrk-kpa/suomifi-ui-components.git"
  },
  "author": "Joonas Kallunki <jkallunki@gmail.com> (https://jkallunki.net/)",
  "contributors": [
    "Aappo Ã…lander",
    "Joonas Kaski",
    "Sami Korpela",
    "Valtteri Laukkanen"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vrk-kpa/suomifi-ui-components/issues"
  },
  "homepage": "https://github.com/vrk-kpa/suomifi-ui-components#readme",
  "keywords": [
    "react",
    "suomifi",
    "styleguide",
    "styled-components",
    "typescript",
    "UI",
    "accessibility",
    "a11y"
  ],
  "scripts": {
    "prepare": "npm-run-all husky:install build",
    "husky:install": "husky install",
    "pre-commit": "lint-staged",
    "pre-push": "yarn validate",
    "start": "yarn styleguide",
    "start:withaxe": "cross-env AXE=\"true\" yarn styleguidist server",
    "test": "jest test src --passWithNoTests",
    "test:updatesnapshots": "jest test --u",
    "lint": "run-p lint:eslint lint:stylelint",
    "lint:eslint": "eslint src",
    "lint:stylelint": "stylelint \"./src/**/*.{ts,tsx}\"",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "prettier:check": "prettier-check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "validate": "run-p test lint prettier:check",
    "build": "npm-run-all build:clean build:rollup build:copycss build:copytypes build:cleantmp",
    "build:clean": "rimraf ./dist",
    "build:rollup": "rollup -c",
    "build:copycss": "copyfiles -f ./dist/tmp/main.css ./dist",
    "build:copytypes": "copyfiles -f ./dist/tmp/index.d.ts ./dist/types",
    "build:cleantmp": "rimraf ./dist/tmp",
    "bundle-analyzer": "size-limit --why",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "deploy": "gh-pages -d styleguide -m \"[skip ci] Updates\"",
    "deploy-dev": "gh-pages -d styleguide -e dev -m \"[skip ci] Updates\""
  },
  "devDependencies": {
    "@babel/core": "7.16.10",
    "@babel/plugin-transform-runtime": "7.16.10",
    "@babel/preset-env": "7.16.10",
    "@babel/preset-react": "7.16.7",
    "@reach/rect": "0.16.0",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-node-resolve": "13.1.3",
    "@stylelint/postcss-css-in-js": "0.37.2",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@types/classnames": "2.3.1",
    "@types/jest": "27.4.0",
    "@types/jest-axe": "3.5.3",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/styled-components": "5.1.20",
    "@types/warning": "3.0.0",
    "@typescript-eslint/eslint-plugin": "5.10.0",
    "@typescript-eslint/parser": "5.10.0",
    "autoprefixer": "10.4.2",
    "babel-jest": "27.4.6",
    "clean-css": "5.2.2",
    "clipboard-copy": "4.0.1",
    "copyfiles": "2.4.1",
    "cross-env": "7.0.3",
    "css-loader": "5.2.7",
    "cssnano": "5.0.15",
    "eslint": "8.7.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-react-app": "7.0.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "25.7.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-tree-shaking": "1.10.0",
    "gh-pages": "3.2.3",
    "husky": "7.0.4",
    "jest": "27.4.7",
    "jest-axe": "5.0.1",
    "jest-styled-components": "7.0.8",
    "lint-staged": "12.2.1",
    "node-sass": "7.0.1",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.5",
    "postcss-import": "14.0.2",
    "postcss-syntax": "0.36.2",
    "prettier": "2.5.1",
    "prettier-check": "2.0.0",
    "prettier-eslint": "13.0.0",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-axe": "3.5.4",
    "react-docgen": "5.4.0",
    "react-docgen-typescript": "2.2.2",
    "react-dom": "17.0.2",
    "react-styleguidist": "11.2.0",
    "rimraf": "3.0.2",
    "rollup": "2.64.0",
    "rollup-plugin-postcss": "4.0.2",
    "rollup-plugin-progress": "1.1.2",
    "rollup-plugin-ts": "2.0.5",
    "sass": "1.49.0",
    "sass-loader": "10.2.0",
    "size-limit": "5.0.5",
    "style-loader": "2.0.0",
    "styled-components": "5.3.3",
    "stylelint": "14.2.0",
    "stylelint-config-recommended": "6.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-custom-processor-loader": "0.6.0",
    "stylelint-processor-styled-components": "1.10.0",
    "ts-jest": "27.1.3",
    "ts-loader": "8.1.0",
    "tslib": "2.3.1",
    "typescript": "4.5.4",
    "webpack": "4.46.0"
  },
  "dependencies": {
    "@babel/runtime": "7.16.7",
    "@popperjs/core": "2.11.2",
    "@reach/listbox": "0.16.2",
    "@reach/menu-button": "0.16.2",
    "@reach/popover": "0.16.2",
    "@types/react-modal": "3.13.1",
    "classnames": "2.3.1",
    "polished": "4.1.3",
    "react-modal": "3.14.4",
    "react-popper": "2.2.5",
    "react-svg": "14.1.8",
    "suomifi-design-tokens": "3.2.0",
    "suomifi-icons": "^6.1.0"
  },
  "peerDependencies": {
    "@types/styled-components": ">=5.1.9",
    "@types/warning": ">=3.0.0",
    "react": ">=16.8.0",
    "styled-components": ">=5.2.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": "prettier-check",
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint",
      "stylelint"
    ]
  },
  "size-limit": [
    {
      "path": "dist/suomifi-ui-components.esm.js"
    }
  ]
}

